<package>
  <name>nodelet_tutorial_math</name>
  <version>0.1.7</version>
  <description>Package for Nodelet tutorial.</description>
  <author email="tfoote@willowgarage.com">Tully Foote</author>
  <maintainer email="tfoote@willowgarage.com">Tully Foote</maintainer>
  <maintainer email="iisaito@kinugarage.com">Isao Isaac Saito</maintainer>
  <license>BSD</license>
  <url type="website">http://www.ros.org/wiki/nodelet_tutorial_math</url>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>nodelet</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>class_loader</build_depend>

  <run_depend>nodelet</run_depend>
  <run_depend>roscpp</run_depend>
  <run_depend>std_msgs</run_depend>

  <export>
    <nodelet plugin="${prefix}/nodelet_math.xml"/>
  </export>
</package>


